{
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint:recommanded",
    "plugin:react/recommanded"
  ],
  "plugins": [
    "import",
    "react-hooks"
  ],
  "rules": {
    "indent":[
      "error",
      4
    ],
    "no-unused-vars": 1,
    "no-use-before-define": 1,
    "no-redeclare": 1,
    "no-console":0
  }
}